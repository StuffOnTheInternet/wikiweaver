{
    "manifest_version": 3,
    "name": "WikiWeaver",
    "version": "0.1",
    "description": "Send wikipedia urls to a WikiWeaver server",
    "icons": {
        "48": "icons/48.png",
        "96": "icons/96.png"
    },
    "host_permissions": [
        "*://*.wikipedia.org/wiki/*",
        "*://localhost/*"
    ],
    "permissions": [
        "webNavigation"
    ],
    "background": {
        "service_worker": "service_worker.js"
    }
}